{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
      "default": {
          "format": "%(asctime)s | %(levelname)s | %(name)s:%(module)s:%(lineno)s %(funcName)s: %(message)s"
      }
  },
  "handlers": {
      "console": {
          "class": "logging.StreamHandler",
          "level": "INFO",
          "formatter": "default"
      }
  },
  "loggers": {
      "doru": {
          "level": "INFO",
          "handlers": ["console"],
          "propagate": false
      }
  }
}
